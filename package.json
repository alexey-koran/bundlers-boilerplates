{
  "name": "webpack-boilerplate",
  "version": "1.0.3",
  "description": "Webpack react and typescript boilerplate",
  "main": "src/index.tsx",
  "author": "Alexey-Koran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexey-koran/webpack-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-koran/webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack serve --color --hot --mode=development",
    "prod": "webpack serve --color --hot --mode=production",
    "clean": "rm -f -r build",
    "buildapp": "webpack --mode=production",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "webpack --analyze --mode=production",
    "prepare": "husky install",
    "prettier": "prettier --check \"**/*.+(js|cjs|jsx|ts|tsx)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|cjs|jsx|ts|tsx)\"",
    "lint": "eslint src --ext .js,.cjs,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.cjs,.jsx,.ts,.tsx --fix",
    "lint:fix-staged": "eslint --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "devDependencies": {
    "@svgr/core": "^6.5.1",
    "@svgr/webpack": "^6.5.1",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "clsx": "^1.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.20",
    "postcss-assets": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-clean-order": "^2.2.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
