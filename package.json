{
  "name": "react-template",
  "version": "0.1.5",
  "description": "React and typescript template",
  "main": "src/index.tsx",
  "author": "Alexey-Koran <alexey.koran@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-koran/react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-koran/react-template.git"
  },
  "homepage": "https://github.com/alexey-koran/react-template#readme",
  "scripts": {
    "dev": "webpack serve --color --hot --mode=development",
    "prod": "webpack serve --color --hot --mode=production",
    "clean": "rm -f -r build",
    "buildapp": "webpack --mode=production",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "webpack --analyze --mode=production",
    "prepare": "husky install",
    "prettier": "prettier --check \"**/*.+(js|cjs|jsx|ts|tsx|pcss)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|cjs|jsx|ts|tsx|pcss)\"",
    "lint": "eslint **/*.{js,mjs,cjs,jsx,mjsx,ts,mts,cts,tsx,mtsx}",
    "lint:fix": "eslint **/*.{js,mjs,cjs,jsx,mjsx,ts,mts,cts,tsx,mtsx} --fix",
    "lint:fix-staged": "eslint --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "devDependencies": {
    "@alexey-koran/prettier-config": "^0.0.2",
    "@alexey-koran/tsconfig": "^0.0.2",
    "@eslint/js": "^8.36.0",
    "@svgr/webpack": "^6.5.1",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "clsx": "^1.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^2.21.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "globals": "^13.20.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-assets": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "react-error-boundary": "^3.1.4",
    "style-loader": "^3.3.2",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^2.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^2.0.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
