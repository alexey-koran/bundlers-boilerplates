{
  "name": "react-template",
  "version": "0.3.5",
  "description": "React and typescript template",
  "main": "src/index.tsx",
  "type": "module",
  "author": "Alexey-Koran <alexey.koran@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-koran/react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-koran/react-template.git"
  },
  "homepage": "https://github.com/alexey-koran/react-template#readme",
  "scripts": {
    "dev": "node --import ./ts-node.register.mjs node_modules/webpack/bin/webpack serve --color --hot --mode=development",
    "prod": "node --import ./ts-node.register.mjs node_modules/webpack/bin/webpack serve --color --hot --mode=production",
    "buildapp": "node --import ./ts-node.register.mjs node_modules/webpack/bin/webpack --mode=production",
    "clean": "rm -f -r build",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "node --import ./ts-node.register.mjs node_modules/webpack/bin/webpack --analyze --mode=production",
    "prepare": "husky && echo 'pnpm lint-staged' > .husky/pre-commit",
    "prettier": "prettier --check \"**/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx,pcss}\"",
    "prettier:fix": "prettier --write \"**/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx,pcss}\"",
    "lint": "eslint **/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx}",
    "lint:fix": "eslint **/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx} --fix",
    "lint:fix-staged": "eslint --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "test:eslint-config": "eslint-config-inspector",
    "test:webpack-config": "node --import ./ts-node.register.mjs node_modules/webpack/bin/webpack configtest",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "devDependencies": {
    "@alexey-koran/prettier-config": "^0.1.1",
    "@alexey-koran/tsconfig": "^0.1.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/config-inspector": "^0.5.2",
    "@eslint/js": "^9.8.0",
    "@stylistic/eslint-plugin-js": "2.6.1",
    "@stylistic/eslint-plugin-jsx": "2.6.1",
    "@stylistic/eslint-plugin-ts": "2.6.1",
    "@svgr/webpack": "^8.1.0",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/eslint__js": "^8.42.3",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "clsx": "^2.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-06d0b89e-20240801",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.40",
    "postcss-assets": "^6.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react-error-boundary": "^4.0.13",
    "style-loader": "^4.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "svgo": "4.0.0-rc.0",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.0.0",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
