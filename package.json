{
  "name": "react-template",
  "version": "0.2.0",
  "description": "React and typescript template",
  "main": "src/index.tsx",
  "type": "module",
  "author": "Alexey-Koran <alexey.koran@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-koran/react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-koran/react-template.git"
  },
  "homepage": "https://github.com/alexey-koran/react-template#readme",
  "scripts": {
    "dev": "node --loader ts-node/esm node_modules/webpack/bin/webpack serve --color --hot --mode=development",
    "prod": "node --loader ts-node/esm node_modules/webpack/bin/webpack serve --color --hot --mode=production",
    "buildapp": "node --loader ts-node/esm node_modules/webpack/bin/webpack --mode=production",
    "clean": "rm -f -r build",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "webpack --analyze --mode=production",
    "prepare": "husky install",
    "prettier": "prettier --check \"**/*.+(js|cjs|jsx|ts|tsx|pcss)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|cjs|jsx|ts|tsx|pcss)\"",
    "lint": "eslint **/*.{js,mjs,cjs,jsx,mjsx,ts,mts,cts,tsx,mtsx}",
    "lint:fix": "eslint **/*.{js,mjs,cjs,jsx,mjsx,ts,mts,cts,tsx,mtsx} --fix",
    "lint:fix-staged": "eslint --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "devDependencies": {
    "@alexey-koran/prettier-config": "^0.0.2",
    "@alexey-koran/tsconfig": "^0.0.6",
    "@eslint/js": "^8.47.0",
    "@svgr/webpack": "^8.1.0",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "clsx": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^4.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-import-resolver-webpack": "^0.13.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "globals": "^13.21.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.28",
    "postcss-assets": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "react-error-boundary": "^4.0.11",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
